            <pair start="-2.7,-2.7" end="3,3" mark="" />
            <copy of="C" />
        </stitch>
        <stitch id="ll" pairs="8" display="none">
            <title>
                <value lang="en">triple twist</value>
                <value lang="nl">dubbele draai</value>
            </title>
            <style width="2" />
            <pair start="-2.7,-2.7" end="3,3" mark="" />
            <copy of="B" />
            <copy of="C" />
        </stitch>
        <stitch id="lll" pairs="9" display="none">
            <title>
                <value lang="en">triple twist</value>
                <value lang="nl">driedubbele draai</value>
            </title>
            <style width="2" />
            <pair start="-2.7,-2.7" end="3,3" mark="" />
            <copy of="A" />
            <copy of="B" />
            <copy of="C" />
        </stitch>
        <stitch id="r" pairs="9" display="none">
