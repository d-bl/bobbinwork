    <group pairs="2" display="no">

        <title>
            <value lang="en">stitches</value>
            <value lang="nl">slagen</value>
        </title>
        <stitch pairs="25-27">
          <title><value lang="en"></value></title>
          <style width="2" />
          <pair start="0,0" end="0,0" />
          <pair start="0,0" end="0,0" />
          <pair start="0,0" end="0,0" />
          <cross id="C1" bobbins="2-3">
            <back start="3,-1" c1="2,0" c2="-2,0"  end="-3,1" />
            <front start="-3,-1" c1="-2,0" c2="2,0" end="3,1" />
          </cross>
          <cross id="C2" bobbins="4-5">
            <back start="3,-1" c1="2,0" c2="-2,0"  end="-3,1" />
            <front start="-3,-1" c1="-2,0" c2="2,0" end="3,1" />
          </cross>
          <twist id="T1" bobbins="1-2">
            <back start="-3,-1" c1="-2,0" c2="2,0"  end="3,1" />
            <front start="3,-1" c1="2,0" c2="-2,0" end="-3,1" />
          </twist>
          <twist id="T2" bobbins="3-4">
            <back start="-3,-1" c1="-2,0" c2="2,0"  end="3,1" />
            <front start="3,-1" c1="2,0" c2="-2,0" end="-3,1" />
          </twist>
          <twist id="T3" bobbins="5-6">
            <back start="-3,-1" c1="-2,0" c2="2,0"  end="3,1" />
            <front start="3,-1" c1="2,0" c2="-2,0" end="-3,1" />
          </twist>
        </stitch>
        <group id="star" pairs="28-30">
          <title><value lang="en">Three pair star</value></title>
          <title><value lang="nl">Sterretje</value></title>
          <stitch pairs="1-3">
            <title><value lang="en">Twist all pairs</value></title>
            <title><value lang="nl">Alle paren draaien</value></title>
              <pair start="-14,-8"  end="0,0" />
              <pair start="0,-18" end="0,0" />
              <pair start="14,-8" end="0,0" />
              <copy of="T1"><move y="-22"  /><rotate centre="0,0" angle="-60" /></copy>
              <copy of="T2"><move y="-22"  /><rotate centre="0,0" angle="0" /></copy>
              <copy of="T3"><move y="-22"  /><rotate centre="0,0" angle="60" /></copy>
          </stitch>
          <stitch pairs="1-3">
            <title><value lang="en">Inverted cloth stitch</value></title>
            <title><value lang="nl">omgekeerde linnenslag</value></title>
              <pair start="0,0"  end="0,0" />
              <pair start="0,0" end="0,0" />
              <pair start="0,0" end="0,0" />
              <copy of="C1"><move y="-17" /><rotate centre="0,0" angle="-30" /></copy>
              <copy of="C2"><move y="-17" /><rotate centre="0,0" angle="30" /></copy>
              <copy of="T2"><move y="-10"  /><rotate centre="0,0" angle="0" /></copy>
              <copy of="C1"><move y="-10" /><rotate centre="0,0" angle="-60" /></copy>
              <copy of="C2"><move y="-10" /><rotate centre="0,0" angle="60" /></copy>
          </stitch>
          <stitch pairs="1-3">
            <title><value lang="en">Twist outer pairs</value></title>
            <title><value lang="nl">Buitenste paren draaien</value></title>
              <pair start="0,0"  end="0,0" />
              <pair start="0,0" end="0,0" />
              <pair start="0,0" end="0,0" />
              <copy of="T1"><move y="0" x="-17" /><rotate centre="0,0" angle="0" /></copy>
              <copy of="T3"><move y="0" x="17" /><rotate centre="0,0" angle="0" /></copy>
        
          </stitch>
          <stitch pairs="1-3">
            <title><value lang="en">Inverted cloth stitch</value></title>
            <title><value lang="nl">omgekeerde linnenslag</value></title>
              <pair start="0,0"  end="0,0" />
              <pair start="0,0" end="0,0" />
              <pair start="0,0" end="0,0" />
              <copy of="C1"><move y="10" /><rotate centre="0,0" angle="60" /></copy>
              <copy of="C2"><move y="10" /><rotate centre="0,0" angle="-60" /></copy>
    
              <copy of="T2"><move y="10"  /><rotate centre="0,0" angle="0" /></copy>
    
              <copy of="C1"><move y="17" /><rotate centre="0,0" angle="30" /></copy>
              <copy of="C2"><move y="17" /><rotate centre="0,0" angle="-30" /></copy>
          </stitch>
          <stitch pairs="1-3">
            <title><value lang="en">Twist all pairs</value></title>
            <title><value lang="nl">Alle paren draaien</value></title>
              <pair start="0,0"  end="14,8" />
              <pair start="0,0" end="0,18" />
              <pair start="0,0" end="-14,8" />
              <copy of="T1"><move y="22"/><rotate centre="0,0" angle="60" /></copy>
              <copy of="T2"><move y="22"/><rotate centre="0,0" angle="0" /></copy>
              <copy of="T3"><move y="22"/><rotate centre="0,0" angle="-60" /></copy>
            </stitch>
        </group>
        <stitch id="ctctc" pairs="1-2">
            <title>
                <value lang="en">fixing stitch</value>
                <value lang="nl">fixeerslag</value>
            </title>
            <style width="2" />
            <pair start="0,6.6" end="16.6,23.3" />
            <pair start="16.6,6.6" end="0,23.3" />
            <cross id="G" bobbins="2-3">
                <back start="10,0" end="5,5" />
                <front start="6.6,0" end="11.6,5" />
            </cross>
            <twist id="H" bobbins="1-2">
                <back start="0,6.6" end="5,11.6" />
                <front start="5,5" c1="0,10" c2="0,10" end="0,15" />
            </twist>
            <twist id="I" bobbins="3-4">
                <back start="11.6,5" c1="16.6,10" c2="16.6,10" end="16.6,15" />
                <front start="16.6,6.6" end="11.6,11.6" />
            </twist>
            <cross id="J" bobbins="2-3">
                <back start="11.6,11.6" end="5,18.3" />
                <front start="5,11.6" end="11.6,18.3" />
            </cross>
            <twist id="K" bobbins="1-2">
                <back start="0,15" c1="0,20" c2="0,20" end="5,25" />
                <front start="5,18.3" end="0,23.3" />
            </twist>
            <twist id="L" bobbins="3-4">
                <back start="11.6,18.3" end="16.6,23.3" />
                <front start="16.3,15" c1="16.3,20" c2="16.3,20" end="11.6,25" />
            </twist>
            <cross id="M" bobbins="2-3">
                <back start="11.6,25" end="5,31.6" />
                <front start="5,25" end="11.6,31.6" />
            </cross>
        </stitch>
        <stitch id="cttc" pairs="3-4">
            <title>
                <value lang="en">turning stitch</value>
                <value lang="nl">omkeerslag</value>
            </title>
            <style width="2" />
            <pair start="0,6.6" end="16.6,23.3" />
            <pair start="16.6,6.6" end="0,23.3" />
            <copy of="G" />
            <copy of="H" />
            <copy of="I" />
            <copy of="K" />
            <copy of="L" />
            <copy of="M" />
        </stitch>

        <stitch id="crtc" pairs="5-6">
            <title>
                <value lang="en">turn even threads</value>
                <value lang="nl">even draden keren</value>
            </title>
            <style width="2" />
            <pair start="16.6,6.6" end="0,23.3" />
            <pair start="0,6.6" end="16.6,23.3" />
            <copy of="G" />
            <copy of="I" />
            <copy of="L" />
            <twist id="S" bobbins="1-2">
                <back start="0,6.6" c1="2,8.6" c2="3,23" end="5,25" />
                <front start="5,5" c1="0,10" c2="3,20" end="0,23.3" />
            </twist>
            <copy of="M" />
        </stitch>

        <stitch id="cltc" pairs="7-8">
            <title>
                <value lang="en">turn odd threads</value>
                <value lang="nl">oneven draden keren</value>
            </title>
            <style width="2" />
            <pair start="16.6,6.6" end="0,23.3" />
            <pair start="0,6.6" end="16.6,23.3" />
            <copy of="G" />
            <copy of="H" />
            <copy of="K" />
            <twist id="T" bobbins="3-4">
                <back start="11.6,5" c1="16.6,10" c2="7,11" end="16.6,23.3" />
                <front start="16.6,6.6" c1="11.6,12.6" c2="13.6,23" end="11.6,25" />
            </twist>
            <copy of="M" />
        </stitch>

        <stitch id="ctc" pairs="9-10">
            <title>
                <value lang="en">cloth stitch</value>
                <value lang="nl">linnenslag</value>
            </title>
            <style color="#AA00FF" width="2" />
            <pair start="3,3" end="13.5,13.5" />
            <pair start="13.5,3" end="3,13.3" />
            <copy of="G" />
            <twist id="N" bobbins="1-2">
                <back start="0,6.6" end="5,11.6" />
                <front start="5,5" end="0,10" />
            </twist>
            <twist id="O" bobbins="3-4">
                <back start="11.6,5" end="16.6,10" />
                <front start="16.6,6.6" end="11.6,11.6" />
            </twist>
            <cross id="P" bobbins="2-3">
                <back start="11.6,11.6" end="6.6,16.6" />
                <front start="5,11.6" end="10,16.3" />
            </cross>
        </stitch>

        <stitch id="tc" pairs="15-16">
            <title>
                <value lang="en">half stitch (open)</value>
                <value lang="nl">netslag (open)</value>
            </title>
            <style width="2" color="#00BB00" />
            <pair start="3,13.3" end="13.5,3" />
            <pair start="3,3" end="13.5,13.5" />
            <twist id="Q" bobbins="1-2">
                <back start="0,6.6" end="5,11.6" />
                <front start="5,2" c1="5,5" c2="5,5" end="0,10" />
            </twist>
            <twist id="R" bobbins="3-4">
                <back start="11.6,2" c1="11.6,5" c2="11.6,5" end="16.6,10" />
                <front start="16.6,6.6" end="11.6,11.6" />
            </twist>
            <copy of="P" />
        </stitch>

        <stitch id="crc" pairs="11-12">
            <title>
                <value lang="en">no left twist</value>
                <value lang="nl">geen linker draai</value>
            </title>
            <pair start="3,13.3" end="13.5,3" />
            <pair start="3,3" end="13.5,13.5" />
            <copy of="G" />
            <copy of="O" />
            <copy of="P" />
        </stitch>

        <stitch id="clc" pairs="13-14">
            <title>
                <value lang="en">no right twist</value>
                <value lang="nl">geen rechter draai</value>
            </title>
            <pair start="3,13.3" end="13.5,3" />
            <pair start="3,3" end="13.5,13.5" />
            <copy of="G" />
            <copy of="N" />
            <copy of="P" />
        </stitch>

        <stitch id="tc_ltr" pairs="17-18">
            <title>
                <value lang="en">half Stitch (open) - weaving from left to
                    right</value>
                <value lang="nl">netslag (open) - loper van links naar
                    rechts</value>
            </title>
            <style color="#009933" width="2" />
            <pair start="8,15" end="22,15" />
            <pair start="15,8" end="15,22" />
            <twist bobbins="1-2">
                <back start="-4,20" end="08,20" />
                <front start="00,15" end="06,27" />
            </twist>
            <twist bobbins="3-4">
                <back start="18,03" end="24,17" />
                <front start="26,03" end="18,17" />
            </twist>
            <cross bobbins="2-3">
                <back start="18,17" end="11,27" />
                <front start="08,20" end="23,20" />
            </cross>
        </stitch>

        <stitch id="tc_rtl" pairs="19-20">
            <title>
                <value lang="en">half Stitch (open) - weaving from right to
                    left</value>
                <value lang="nl">netslag (open) - loper van rechts naar
                    links</value>
            </title>
            <style color="#009933" width="2" />
            <pair start="15,8" end="15,22" />
            <pair start="8,15" end="22,15" />
            <twist bobbins="3-4">
                <back start="29,15" end="22,28" />
                <front start="34,20" end="19,20" />
            </twist>
            <twist bobbins="1-2">
                <back start="04,02" end="12,15" />
                <front start="12,02" end="04,15" />
            </twist>
            <cross bobbins="2-3">
                <back start="19,20" end="08,20" />
                <front start="12,15" end="16,28" />
            </cross>
        </stitch>
        <stitch id="tttctctc" pairs="21-22">
            <title>
                <value lang="en">triple twist, fixing stitch</value>
                <value lang="nl">driedubbele draai, fixeerslag</value>
            </title>
            <pair start="-8.7,-8.7" end="16.6,23.3" />
            <pair start="25.3,-8.7" end="0,23.3" />
            <twist id="A" bobbins="1-2">
                <back start="-18,-11.4" c1="-12,-5.4" c2="-11.4,-18" end="-5.4,-12" />
                <front start="-11.4,-18" c1="-5.4,-12" c2="-18,-11.4" end="-12,-5.4" />
            </twist>
            <twist id="B" bobbins="1-2">
                <back start="-12,-5.4" c1="-6,0.6" c2="-5.4,-12" end="0.6,-6" />
                <front start="-5.4,-12" c1="0.6,-6" c2="-12,-5.4" end="-6,0.6" />
            </twist>
            <twist id="C" bobbins="1-2">
                <back start="-6,0.6" c1="0,6.6" c2="0.6,-6" end="6.6,0" />
                <front start="0.6,-6" c1="6.6,0" c2="-6,0.6" end="0,6.6" />
            </twist>
            <twist id="D" bobbins="3-4">
                <back start="28,-18" c1="22,-12" c2="34.6,-11.4" end="28.6,-5.4" />
                <front start="34.6,-11.4" c1="28.6,-5.4" c2="28,-18" end="22,-12" />
            </twist>
            <twist id="E" bobbins="3-4">
                <back start="22,-12" c1="16,-6" c2="28.6,-5.4" end="22.6,0.6" />
                <front start="28.6,-5.4" c1="22.6,0.6" c2="22,-12" end="16,-6" />
            </twist>
            <twist id="F" bobbins="3-4">
                <back start="16,-6" c1="10,0" c2="22.6,0.6" end="16.6,6.6" />
                <front start="22.6,0.6" c1="16.6,6.6" c2="16,-6" end="10,0" />
            </twist>
            <copy of="G" />
            <copy of="H" />
            <copy of="I" />
            <copy of="J" />
            <copy of="K" />
            <copy of="L" />
            <copy of="M" />
        </stitch>
        <stitch id="tctc" pairs="23-24">
            <title>
                <value lang="en">twist, cloth stitch</value>
                <value lang="nl">dubbele netslag</value>
            </title>
            <style color="#FF0000" width="2" />
            <pair start="3,3" end="13.5,13.5" />
            <pair start="13.5,3" end="3,13.3" />
            <copy of="C" />
            <copy of="F" />
            <copy of="G" />
            <copy of="N" />
            <copy of="O" />
            <copy of="P" />
        </stitch>
        <group id="vg" pairs="31-32">
          <title>
            <value lang="en">Torchon</value>
            <value lang="nl">Vetergatslag</value>
          </title>
          <stitch pairs="1-2">
            <title>
              <value lang="en">half stitch</value>
              <value lang="nl">netslag</value>
            </title>
            <style color="#00FF00" width="2" />
            <pair start="3,3" end="8.5,4.5" />
            <pair start="13.5,3" end="8.5,4.5" />
            <copy of="C"/>
            <copy of="F"/>
            <copy of="G"/>
          </stitch>
          <pin position="8.5,8.5"/>
          <stitch pairs="1-2">
            <title>
              <value lang="en">half stitch</value>
              <value lang="nl">netslag</value>
            </title>
            <style color="#00FF00" width="2" />
            <pair start="8.5,12" end="13.5,13.5" />
            <pair start="8.5,12" end="3,13.3" />
            <copy of="N"/>
            <copy of="O"/>
            <copy of="P"/>
          </stitch>
        </group>
    </group>
    <group pairs="3" display="no">
        <title>
            <value lang="en">loose twists</value>
