<?xml version="1.0" encoding="UTF-8"?>
<diagram
  xmlns="http://BobbinWork.googlecode.com/bw.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://BobbinWork.googlecode.com bw.xsd"
  xmlns:xi="http://www.w3.org/2001/XInclude"
>
  <title>
    <value lang="en">Copyright 2009, J. Pol, some rights reserved: http://creativecommons.org/licenses/by/3.0/</value>
  </title>
  <group pairs="1" display="no">
    <title>
        <value lang="en">Library (building blocks for the diagram)</value>
        <value lang="nl">Bibliotheek (bouwstenen voor de werktekening)</value>
        <value lang="de">Bibliothek (Bausteine fur Arbeitszeichnungen)</value>
    </title>
    <stitch pairs="1-2">
      <title><value lang="en"></value></title>
      <style width="2" />
      <pair start="0,0" end="0,0" />
      <pair start="0,0" end="0,0" />
      <cross id="starC1" bobbins="2-3">
        <front start="-5,-2" c1="-3,0" c2="3,0" end="5,2" />
        <back start="5,-2" c1="3,0" c2="-3,0"  end="-5,2" />
      </cross>
      <cross id="starC2" bobbins="4-5">
        <front start="-5,-2" c1="-3,0" c2="3,0" end="5,2" />
        <back start="5,-2" c1="3,0" c2="-3,0"  end="-5,2" />
      </cross>
      <twist id="starT1" bobbins="1-2">
        <front start="5,-2" c1="3,0" c2="-3,0" end="-5,2" />
        <back start="-5,-2" c1="-3,0" c2="3,0"  end="5,2" />
      </twist>
      <twist id="starT2" bobbins="3-4">
        <front start="5,-2" c1="3,0" c2="-3,0" end="-5,2" />
        <back start="-5,-2" c1="-3,0" c2="3,0"  end="5,2" />
      </twist>
      <twist id="starT3" bobbins="5-6">
        <front start="5,-2" c1="3,0" c2="-3,0" end="-5,2" />
        <back start="-5,-2" c1="-3,0" c2="3,0"  end="5,2" />
      </twist>
    </stitch>
    <stitch id="star" pairs="3-5">
      <pair start="-25,-15"  end="25,15" />
      <pair start="0,-30" end="0,30" />
      <pair start="25,-15" end="-25,15" />

      <copy of="starT1"><move y="-30"  /><rotate centre="0,0" angle="-60" /></copy>
      <copy of="starT2"><move y="-30"  /><rotate centre="0,0" angle="0" /></copy>
      <copy of="starT3"><move y="-30"  /><rotate centre="0,0" angle="60" /></copy>
  
      <copy of="starC1"><move y="-25" /><rotate centre="0,0" angle="-30" /></copy>
      <copy of="starC2"><move y="-25" /><rotate centre="0,0" angle="30" /></copy>
      <copy of="starT2"><move y="-15"  /><rotate centre="0,0" angle="0" /></copy>
      <copy of="starC1"><move y="-15" /><rotate centre="0,0" angle="-60" /></copy>
      <copy of="starC2"><move y="-15" /><rotate centre="0,0" angle="60" /></copy>
  
      <copy of="starT1"><move y="0" x="-25" /><rotate centre="0,0" angle="0" /></copy>
      <copy of="starT3"><move y="0" x="25" /><rotate centre="0,0" angle="0" /></copy>

      <copy of="starC1"><move y="15" /><rotate centre="0,0" angle="60" /></copy>
      <copy of="starC2"><move y="15" /><rotate centre="0,0" angle="-60" /></copy>
      <copy of="starT2"><move y="15"  /><rotate centre="0,0" angle="0" /></copy>
      <copy of="starC1"><move y="25" /><rotate centre="0,0" angle="30" /></copy>
      <copy of="starC2"><move y="25" /><rotate centre="0,0" angle="-30" /></copy>

      <copy of="starT1"><move y="30"/><rotate centre="0,0" angle="60" /></copy>
      <copy of="starT2"><move y="30"/><rotate centre="0,0" angle="0" /></copy>
      <copy of="starT3"><move y="30"/><rotate centre="0,0" angle="-60" /></copy>
    </stitch>
  </group>
  <group pairs="2">
    <title>
        <value lang="en">diagram</value>
        <value lang="nl">werktekening</value>
        <value lang="de">Arbeitszeichnung</value>
    </title>
    <group pairs="1-12">
      <new_bobbins nrs="5,6,15,16,19,20,21,22"><style color="#0000FF"></style></new_bobbins>
      <new_bobbins nrs="9,10,11,12,13,14"><style color="#00FF00"></style></new_bobbins>
      <new_bobbins nrs="1,2,3,4,7,8,17,18"><style color="#FF0000"></style></new_bobbins>
      <copy of="star" pairs="5-7"><move x="165" y="35"/></copy>

      <copy of="star" pairs="3-5"><move x="100" y="75"/></copy>
      <copy of="star" pairs="7-9"><move x="230" y="75"/></copy>

      <copy of="star" pairs="1-3"><move x="35" y="115"/></copy>
      <copy of="star" pairs="5-7"><move x="165" y="115"/></copy>
      <copy of="star" pairs="9-11"><move x="295" y="115"/></copy>

      <copy of="star" pairs="3-5"><move x="100" y="155"/></copy>
      <copy of="star" pairs="7-9"><move x="230" y="155"/></copy>

      <copy of="star" pairs="5-7"><move x="165" y="195"/></copy>
     <!-- 
      -->
    </group>
  </group>
</diagram>