  <title>
    <value lang="en"> Copyright 2006-2008, J. Falkink-Pol, some rights reserved </value>
  </title>
  
  <group id="group" pairs="1-20">
    <title>
      <value lang="en">Diagram</value>
      <value lang="nl">Werktekening</value>
      <value lang="de">Arbeitszeichnung</value>
    </title>
    <new_bobbins nrs="1,3,5,7,9,11,13,15"><style color="#0000FF"></style></new_bobbins>
    <new_bobbins nrs="2,4,6,8,10,12,14,16"><style color="#FF0000"><shadow color="#FFBBBB" width="5"/></style></new_bobbins>
    <group id="diag"  pairs="1-12">
      <group id="flanders"  pairs="7-10">
        <title>
          <value lang="en">Flanders ground</value>
          <value lang="nl">Vlaanderse grond</value>
        </title>
        
        <!-- left upper  --><copy of="tc"  pairs="1-2"><rotate centre="10,10" angle="-45"/><move x="180"/></copy>
        <!-- right upper --><copy of="tc"  pairs="3-4"><rotate centre="10,10" angle="45"/><move x="240"/></copy>
        <!-- top         --><copy of="tc"  pairs="2-3"><move y="10" x="210"/></copy>
        <pin position="218,40"/>
        <!-- left        --><copy of="tc"  pairs="1-2"><move y="30" x="190"/></copy>
        <!-- right       --><copy of="tc"  pairs="3-4"><move y="30" x="230"/></copy>
        <!-- bottom      --><copy of="ctc" pairs="2-3"><move y="50" x="210"/></copy>
        
      </group><!-- flanders -->
      <copy of="flanders" pairs="5-8"><move x="-60" y="60" /></copy>
      <copy of="flanders" pairs="3-6"><move x="-120" y="120" /></copy>
      <copy of="flanders" pairs="1-4"><move x="-180" y="180" /></copy>
    </group><!-- diag -->
    <copy of="diag" pairs="3-12"><move x="60" y="60" /></copy>
    <copy of="diag" pairs="5-14"><move x="120" y="120" /></copy>
    <copy of="diag" pairs="7-16"><move x="180" y="180" /></copy>
  </group>
</diagram>